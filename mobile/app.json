{
  "expo": {
    "name": "PrayChain",
    "slug": "praychain",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "praychain",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.praychain.app",
      "newArchEnabled": false
    },
    "android": {
      "package": "com.praychain.app",
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "newArchEnabled": false
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
